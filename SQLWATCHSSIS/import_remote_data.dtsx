        <SQLTask:SqlTaskData
          SQLTask:Connection="{BB2F3116-D4D3-4816-8822-C4A0CCC7D0ED}"
          SQLTask:SqlStatementSource="/* check data consistency. If, for any reason we have snapshot but not corresponding detail, &#xA;&#x9;we have to delete that snapshot and it will be re-downloaded with the next iteration.&#xA;&#x9;This has to run right after each delta to make sure we're not getting hit but retantion on remote.&#xA;&#x9;RI prevents any details without a valid snapshot.*/&#xA;declare @sql varchar(max) = ''&#xA;select @sql = @sql + '&#xA;delete sh from  [dbo].[sqlwatch_logger_snapshot_header] sh&#xA;left join ' + TABLE_SCHEMA + '.' + TABLE_NAME + ' ud&#xA;&#x9;on sh.snapshot_time = ud.snapshot_time&#xA;&#x9;and sh.snapshot_type_id = ud.snapshot_type_id&#xA;&#x9;and sh.sql_instance = ud.sql_instance&#xA;where ud.snapshot_time is null&#xA;and sh.sql_instance = ?;&#xA;&#xA;delete ud from  [dbo].[sqlwatch_logger_snapshot_header] sh&#xA;right join ' + TABLE_SCHEMA + '.' + TABLE_NAME + ' ud&#xA;&#x9;on sh.snapshot_time = ud.snapshot_time&#xA;&#x9;and sh.snapshot_type_id = ud.snapshot_type_id&#xA;&#x9;and sh.sql_instance = ud.sql_instance&#xA;where sh.snapshot_time is null&#xA;and ud.sql_instance = ?;' from INFORMATION_SCHEMA.TABLES&#xA;where TABLE_NAME like 'sqlwatch_logger%'&#xA;and TABLE_SCHEMA = 'dbo'&#xA;&#xA;exec (@sql)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
